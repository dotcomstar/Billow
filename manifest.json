{
  "name": "Billow",
  "description": "This extension will hide price information for houses on Zillow, replacing it with an input field.",
  "version": "0.2",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
   "activeTab",
   "storage",
   "scripting"
   ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon16.png",
      "32": "/images/icon16.png",
      "48": "/images/icon16.png",
      "128": "/images/icon16.png"
    },
    "icons": {
      "16": "/images/icon16.png",
      "32": "/images/icon16.png",
      "48": "/images/icon16.png",
      "128": "/images/icon16.png"
    }
  }
}
